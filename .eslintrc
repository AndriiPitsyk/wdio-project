{
  "extends": [
      "airbnb/base",
      "plugin:wdio/recommended"
  ],
  "rules": {
    "indent": ["error", 4],
    "func-names": "off",
    "max-len": ["error", { "code": 122 }],
    "comma-dangle": ["error", "never"]
  },
  "plugins": ["wdio"],
  "env": {
    "mocha": true,
    "browser": true
  }
}
